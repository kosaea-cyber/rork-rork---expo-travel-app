{
  "expo": {
    "name": "RAWASI Elite",
    "slug": "rawasi-elite",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rawasi-elite",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.rawasi.elite",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos to upload images.",
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera to upload images.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone if needed for support."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rawasi.elite",
      "permissions": [
        "INTERNET",
        "CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos so you can upload images."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
